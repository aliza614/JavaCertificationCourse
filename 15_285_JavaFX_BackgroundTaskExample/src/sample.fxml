<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<GridPane fx:controller="Controller" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button text="Get Employee Names" onAction="#buttonPressed"
            GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <ListView fx:id="listView" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <ProgressBar fx:id="progressBar" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
</GridPane>